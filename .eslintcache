st.js",["30"],"import React, { useEffect, useState } from 'react'\nimport './post.css';\nimport Avatar from '@material-ui/core/Avatar';\nimport { db, firebase } from './firebase';\n\n\nfunction Post({postId, user, username, imageUrl,caption}) {\nconst [comments, setComments] = useState([]);\nconst[comment, setComment] = useState('');\n\nuseEffect(() => {\n    let unsubscribe;\n    if(postId){\n        unsubscribe = db\n        .collection(\"posts\")\n        .doc(postId)\n        .collection(\"comments\")\n        .orderBy('timestamp', 'desc')\n        .onSnapshot((snapshot) =>{\n         setComments(snapshot.docs.map((doc) => doc.data()));\n        });\n    }\n    return () => {\n       unsubscribe();\n    }\n}, [postId]);\n\nconst postComment = (event) =>{\nevent.preventDefault();\ndb.collection(\"posts\")\n.doc(postId)\n.collection(\"comments\")\n.add({ text: comment,\n     username : user.displayName,\n     timestamp: firebase.firestore.FieldValue.serverTimestamp()\n     });\n\nsetComment('');\n}\n    return (\n        \n        <div className=\"post\">\n       \n            <div className=\"post_header\">\n            <Avatar className=\"post_avatar\"\n            alt=\"Ajinkya\" src=\"/static/images/avatar/1.jpg\" />\n            <h3>{username}</h3>\n            </div>\n            {/* img */}\n       <img  className=\"post_image\"\n        src={imageUrl}/>\n            {/* username: caption */}\n            <h4 className=\"post_text\"><strong>{username}: </strong>{caption}</h4>\n\n            <div className=\"post_comments\">\n              {comments.map((comment) =>(\n                  <p>\n                      <strong>{comment.username}</strong> {comment.text}\n                  </p>\n              ))}\n            </div>\n\n           {user && (\n            <form className=\"post_commentbox\">\n                <input\n                className=\"post_input\"\n                type=\"text\"\n                placeholder=\"Add a comment...\"\n                value={comment}\n                onChange={(e) => setComment(e.target.value)}\n                />\n                <button \n                className=\"post_button\"\n                disabled={!comment}\n                type=\"submit\"\n                onClick={postComment}\n                >\n                    post\n                </button>\n            </form>\n           )}\n            \n        </div>\n    )\n}\n\nexport default Post\n","/Users/ajinkyabhamre/Desktop/Projects/instagram-clone/src/firebase.js",[],"/Users/ajinkyabhamre/Desktop/Projects/instagram-clone/src/ImageUpload.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":50,"column":8,"nodeType":"37","endLine":51,"endColumn":25},"no-native-reassign",["38"],"no-negated-in-lhs",["39"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]